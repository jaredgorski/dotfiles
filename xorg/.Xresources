! copy/paste
*.selectToClipboard: true
*.disallowedWindowIps: 20,21,SetXProp

! font
*faceName:      GohuFont
*faceSize:      12

! special
*.foreground:   #d0d0d0
*.background:   #121212
*.cursorColor:  #d0d0d0

! black
*.color0:       #282828
